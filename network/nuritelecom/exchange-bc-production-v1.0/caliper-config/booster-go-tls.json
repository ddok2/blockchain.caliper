{
  "caliper": {
    "blockchain": "booster",
    "command" : {
      "start": "network/nuritelecom/exchange-bc-production-v1.0/start.sh; docker exec cli bash -c '/opt/gopath/src/github.com/hyperledger/fabric/peer/scripts/newstart.sh exchange-channel 5 10'; sleep 5s; docker build -t txbooster TxBooster/. --no-cache; docker-compose -f Txbooster/booster.yaml up -d; sleep 5s;",
      "end" : "network/nuritelecom/exchange-bc-production-v1.0/stop.sh; docker rm $(docker ps -aqf name=dev-) -f; docker-compose -f Txbooster/booster.yaml down; docker volume rm $(docker volume ls -q); docker rmi $(docker images dev* -q); docker rmi $(docker images txbooster -q);"
    }
  },
  "booster": {
    "hostname": "0.0.0.0",
    "port": "8080"
  },
  "fabric": {
    "cryptodir": "network/nuritelecom/exchange-bc-production-v1.0/crypto-config",
    "network": {
      "orderer": {
        "url": "grpcs://0.0.0.0:17050",
        "mspid": "OrdererMSP",
        "msp": "network/nuritelecom/exchange-bc-production-v1.0/crypto-config/ordererOrganizations/exchange.com/msp/",
        "server-hostname": "orderer1.exchange.com",
        "tls_cacerts": "network/nuritelecom/exchange-bc-production-v1.0/crypto-config/ordererOrganizations/exchange.com/orderers/orderer1.exchange.com/tls/ca.crt"
      },
      "nuriorg": {
        "name": "NuriOrg",
        "mspid": "NuriOrgMSP",
        "msp": "network/nuritelecom/exchange-bc-production-v1.0/crypto-config/peerOrganizations/nuriorg.exchange.com/msp/",
        "ca": {
          "url": "http://0.0.0.0:17054",
          "name": "ca.nuriorg.exchange.com"
        },
        "peer1": {
          "requests": "grpcs://0.0.0.0:7051",
          "events": "grpcs://0.0.0.0:7053",
          "server-hostname": "peer0.nuriorg.exchange.com",
          "tls_cacerts": "network/nuritelecom/exchange-bc-production-v1.0/crypto-config/peerOrganizations/nuriorg.exchange.com/peers/peer0.nuriorg.exchange.com/tls/ca.crt"
        }
      }
    },
    "channel": [
      {
        "name": "exchange-channel",
        "deployed": false,
        "config": "network/nuritelecom/exchange-bc-production-v1.0/channel-artifacts/channel.tx",
        "organizations": [
          "nuriorg"
        ]
      }
    ],
    "chaincodes": [
      {
        "id": "exchange",
        "path": "github.com/chaincode/exchange-ex",
        "language": "golang",
        "version": "1.0",
        "channel": "exchange-channel"
      }
    ],
    "endorsement-policy": {
      "identities": [
        {
          "role": {
            "name": "member",
            "mspId": "NuriOrgMSP"
          }
        },
        {
          "role": {
            "name": "member",
            "mspId": "NFlexOrgMSP"
          }
        }
      ],
      "policy": {
        "2-of": [
          {
            "signed-by": 0
          },
          {
            "signed-by": 1
          }
        ]
      }
    },
    "context": {
      "registerMember": "exchange-channel"
    }
  },
  "info": {
    "Version": "1.4.3",
    "Size": "2 Orgs with 4 Peers with TxBooster",
    "Orderer": "4 Orderers",
    "Distribution": "NURI Telecom Exchange-bc-production with TxBooster"
  }
}
